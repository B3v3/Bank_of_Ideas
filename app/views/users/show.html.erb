<% provide(:title, "#{@user.name}") %>
<h1> Ideas of <%= @user.name %>: </h1>
<ol>
<% @user.ideas.each do |idea| %>
  <li><%= link_to idea.title, idea %></li>
  <% end %>
</ol>
<br />
    <% if (logged_in? && @user.name == current_user.name) %>
<%= link_to 'Logout', logout_path,
    method: :delete %>
    <% end %><br /><br />
<%= render 'layouts/back' %>
